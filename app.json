{
  "expo": {
    "name": "Mixdo",
    "description": "The social todo list",
    "slug": "mixdoprod",
    "privacy": "public",
    "sdkVersion": "30.0.0",
    "platforms": ["ios", "android"],
    "version": "1.1.0",
    "orientation": "portrait",
    "icon": "./assets/mixdoicon.png",
    "facebookScheme": "fb736979746693999",
    "splash": {
      "image": "./assets/opening.png",
      "resizeMode": "contain",
      "backgroundColor": "#7BABED"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mixdo.mixdo"
    },
    "android": {
      "package": "com.mixdo.mixdo",
      "versionCode": 3,
      "googleServicesFile": "./google-services.json"
    },
    "notification": {
      "icon": "./assets/pushicon.png",
      "color": "#ffffff",
      "androidMode": "default"
    }
  }
}
